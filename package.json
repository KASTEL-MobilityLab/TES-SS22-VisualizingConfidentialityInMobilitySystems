{
  "name": "confidentiality-visualizer",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "coverage": "vitest run --coverage --environment jsdom",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "check-lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "check-format": "prettier --ignore-path .gitignore --check \"**/*.+(js|ts|json)\"",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.2",
    "@popperjs/core": "^2.11.5",
    "bootstrap": "^5.2.0",
    "browser-or-node": "^2.0.0",
    "class-transformer": "^0.5.1",
    "leaflet": "^1.8.0",
    "leaflet-routing-machine": "^3.2.12",
    "leaflet.animatedmarker": "^1.0.0",
    "leaflet.motion": "^0.3.2",
    "reflect-metadata": "^0.1.13",
    "vue": "^3.2.37",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@intlify/vite-plugin-vue-i18n": "^5.0.1",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
    "@types/leaflet": "^1.7.11",
    "@types/leaflet-routing-machine": "^3.2.4",
    "@types/node": "^18.6.5",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "c8": "^7.12.0",
    "cross-fetch": "^3.1.5",
    "eslint": "^8.21.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "~4.7.4",
    "vite": "^2.9.14",
    "vitest": "^0.21.1",
    "vue-tsc": "^0.39.5"
  },
  "lint-staged": {
    "*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}": "eslint --ignore-path .gitignore --cache --fix",
    "*.+(js|ts|json)": "prettier --ignore-path .gitignore --write"
  }
}
